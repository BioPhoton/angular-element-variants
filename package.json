{
  "name": "angular-element-vatiants",
  "private": false,
  "scripts": {
    "lerna": "lerna",
    "ng": "ng",
    "start": "ng serve",
    "build:builder": "cd builder-webpack && yarn build",
    "build:core": "cd core && yarn build",
    "build:integration-helper": "cd integration-helper && yarn build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tsc": "tsc",
    "compile:tooling": "tsc",
    "post:compile": "ts-node -P tooling/tsconfig.tooling.json tooling/post-compile.ts",
